  <!--</div>-->
  <!-- Modal Structure -->
  <div id="jh" class="modal" style="width:80%">
    <div class="modal-content">
      <h4>Configuracion de angulos de servos</h4>
      <center>
        <img src="http://localhost:4000/img/confi.png" alt="">
      </center><br>
      <label for="">Servo 1</label>
      <div class="row">
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s1i" ng-blur="blurVA(m_servo.s1i)" >
          <label for="email">Angulo inicial</label>
        </div>
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s1f" ng-blur="blurVA(m_servo.s1f)" >
          <label for="email">Angulo final</label>
        </div>
      </div>
      <label for="">Servo 2</label>
      <div class="row">
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s2i" ng-blur="blurVA(m_servo.s2i)" >
          <label for="email">Angulo inicial</label>
        </div>
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s2f" ng-blur="blurVA(m_servo.s2f)" >
          <label for="email">Angulo final</label>
        </div>
      </div>
      <label for="">Servo 3</label>
      <div class="row">
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s3i" ng-blur="blurVA(m_servo.s3i)" >
          <label for="email">Angulo inicial</label>
        </div>
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s3f" ng-blur="blurVA(m_servo.s3f)" >
          <label for="email">Angulo final</label>
        </div>
      </div>
      <label for="">Servo 4</label>
      <div class="row">
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s4i" ng-blur="blurVA(m_servo.s4i)" >
          <label for="email">Angulo inicial</label>
        </div>
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s4f" ng-blur="blurVA(m_servo.s4f)" >
          <label for="email">Angulo final</label>
        </div>
      </div>
      <label for="">Servo 5</label>
      <div class="row">
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s5i" ng-blur="blurVA(m_servo.s5i)" >
          <label for="email">Angulo inicial</label>
        </div>
        <div class="input-field col s6">
          <input id="number" type="number" class="validate" ng-model="m_servo.s5f" ng-blur="blurVA(m_servo.s5f)" >
          <label for="email">Angulo final</label>
        </div>
      </div>
    </div>
    <div class="modal-footer">
      <a href="#!" class="modal-action waves-effect waves-green btn-flat" ng-click="btnAgregarAngulos(m_servo)" ng-disabled="disabled_input_confi">Aceptar</a>
    </div>
  </div>
