    <div id="control led" ng-init="init()"><br><br>
      <div class="row">
        <div class="col s3">
          <strong>Controles servo 1</strong>
          <label>Rango de giro de {[g_s1i]} a {[g_s1f]}</label>
          <center>
            <img src="http://localhost:4000/img/engine.png">
            <input type="number" ng-model="m_angulo" placeholder="Introduce angulo" ng-blur="blurA(m_angulo,1)">
          </center>
        </div>
        <div class="col s3">
          <strong>Controles servo 2</strong>
          <label>Rango de giro de {[g_s2i]} a {[g_s2f]}</label>
          <center>
